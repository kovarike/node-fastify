NODE_ENV="development"

# Database (Postgres) connection string used by Drizzle ORM
DATABSE_URL="postgresql://postgres:postgres@localhost:5432/postgres"
NODE_ENV_DATABASE="postgresql://postgres:postgres@localhost:5432/postgres"

# JWT & Cookies
NODE_ENV_JWT="your-jwt-env-id"
SECRETET_JWT="your-jwt-secret"
CURRENT_COOKIE_SECRETET="your-cookie-secret"
PREVIOUS_COOKIE_SECRETET_1="previous-cookie-secret-1"
PREVIOUS_COOKIE_SECRETET_2="previous-cookie-secret-2"

# Security & misc
SALT_ROUNDS="10"
ALLOWED_ORIGINS="http://127.0.0.1:3000"
ALLOWED_IP="127.0.0.1"